<div class="clearfix intro-filter filters-section" data-behavior="tooltip" data-position="left" aria-label="Price">
  <h6 class="filter-label col-md-3">Price</h6>
  <div class="price-range-slider">
		<p class="min-price">
		        Min Price:
		        <strong>
		          $<span class="price" id="min-price">
								<% if(minSliderPos){ %>
									<%= minSliderPos * 10 %>
								<% } else { %>
									10
								<% } %>
							</span>
		        </strong>
		      </p>
		<p class="max-price">
		        Max Price:
		        <strong>
		          $<span class="price" id="max-price">
								<% if(maxSliderPos){ %>
									<%= maxSliderPos * 10 %>
								<% } else { %>
									10
								<% } %>
		          </span>
		        </strong>
		      </p>
  </div>
</div>

<div class="clearfix room-type-group intro-filter filters-section" data-behavior="tooltip" data-position="left" aria-label="Room Type">

    <h6 class="filter-label col-md-3">Room Type</h6>

    <div class="btn btn-large btn-flat icon-toggle selected" data-name="Entire home/apt">
      <i class="icon icon-entire-place"></i>
      <div class="h5">Entire Place</div>
    </div>

    <div class="btn btn-large btn-flat icon-toggle" data-name="Private room">
      <i class="icon icon-private-room"></i>
      <div class="h5">Private Room</div>
    </div>

    <div class="btn btn-large btn-flat icon-toggle" data-name="Shared room">
      <i class="icon icon-shared-room"></i>
      <div class="h5">Shared Room</div>
    </div>

    <i class="icon icon-question icon-gray" id="room-type-tooltip"></i>
</div>
<% if(rentals){ %>
	<div class="rentals">
		<% rentals.forEach(function(rental){ %>
			<div class="rental-search-item" data-id="<%= rental.id %>">
				<% var splitAddress = rental.address.split(",") %>
				<% splitAddress.splice(splitAddress.length - 3) %>
				<div class="search-item-header"> <%= splitAddress.join(", ") %> </div>
			 	<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
			 	 	<div class="carousel-inner">
			 	  	<div class="item active">
			 	    	<img src="https://a0.muscache.com/airbnb/static/landing_pages/home_v2/hero/488485-cb8055504a43a9fd3687392c58abe04b.jpg">
			 	  	</div>

			 			<div class="item">
			 				<img src="https://a0.muscache.com/airbnb/static/landing_pages/home_v2/hero/264248-25b69a424e9d6033cd7b417cf8ad4deb.jpg">
			 			</div>
			 		</div>
					<a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
				    <span class="glyphicon glyphicon-chevron-left"></span>
				  </a>
				  <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
				    <span class="glyphicon glyphicon-chevron-right"></span>
				  </a>
				</div>
				<div class="row">
					<div class="col-md-6">
						<div class="search-item-footer"><%= rental.rental_type + " / " + rental.room_type %></div>
					</div>
					<div class="col-md-6">
						<div class="search-item-price"><%= "$" + rental.price + " per night"  %></div>
					</div>
				</div>
			</div>
		<% }) %>
	</div>
<% } %>