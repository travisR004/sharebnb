<div class="home-page-container">
	<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
	  <!-- Indicators -->
	  <ol class="carousel-indicators">
	    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
	    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
	    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
	  </ol>

	  <!-- Wrapper for slides -->
	  <div class="carousel-inner">
	    <div class="item active">
	      <img src="https://a0.muscache.com/airbnb/static/landing_pages/home_v2/hero/488485-cb8055504a43a9fd3687392c58abe04b.jpg">
	    </div>

			<div class="item">
				<img src="https://a0.muscache.com/airbnb/static/landing_pages/home_v2/hero/264248-25b69a424e9d6033cd7b417cf8ad4deb.jpg">
			</div>
	  </div>

	  <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
	    <span class="glyphicon glyphicon-chevron-left"></span>
	  </a>
	  <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
	    <span class="glyphicon glyphicon-chevron-right"></span>
	  </a>

		<div class="intro">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <h2>Plan Your Next Business Trip!</h2>
            <h2>Live away like you live at home </h2>

						<div class="form-group">
							<form id="travel-search" class="form-inline">
								<div id="location-field">
									<input type="text" id="city" class="form-control" name="location[city]" placeholder="Choose your destination...">
								</div>
								<input type="text" id="start-date"class="form-control date" name="location[start_date]" placeholder="Check In">
								<input type="text" id="end-date" class="form-control date" name="location[end_date]" placeholder="Check Out">
								<select class="form-control" name="location[travelers]">
									<% for(var i = 1; i < 6; i ++){ %>
										<option value="<%= i %>"><%= i %> Guests</option>
									<% } %>
									<option value="6">6+ Guests</option>
								</select>
								<button class="btn btn-primary">Find Your Room!</button>
							</form>
						</div>
          </div>
        </div>
      </div>
		</div>


	</div>

	<div id="map-canvas"/>
</div>